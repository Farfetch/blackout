  "devDependencies": {
    "axios": "^0.21.4",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "axios": "^0.21.4",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
